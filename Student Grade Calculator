#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    
    double marks, physics, chemistry, biology, mathematics, computer;
    
    scanf("%lf %lf %lf %lf %lf",&physics,&chemistry,&biology,&mathematics,&computer);
    
    marks = ((physics + chemistry + biology + mathematics + computer)/500) * 100;
    
    printf("Percentage = %.2lf\n",marks);
    
    if(marks>=90){
      printf("Grade = A");  
    } else if(marks>=80 && marks<90){
      printf("Grade = B");  
    } else if(marks>=70 && marks<80){
      printf("Grade = C");  
    } else if(marks>=60 && marks<70){
      printf("Grade = D");  
    } else if(marks>=40 && marks<60){
      printf("Grade = E");  
    } else if(marks<40){
      printf("Grade = F");  
    } 
    
    
    return 0;
}
