#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int number;
    scanf("%d",&number);
    
    if(number == 0){
        printf("Zero");
    } else if (number > 0){
        printf("Positive");
    } else {
        printf("Negative");
    }
    
    return 0;
}
